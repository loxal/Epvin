<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.5/distro-source/core/src/gwt-module.dtd">

<module>
    <inherits name="com.google.gwt.user.UserAgent"/>
    <set-property name="user.agent" value="ie6,ie8,ie9,gecko1_8,safari,opera"/>

    <inherits name="com.google.gwt.i18n.CldrLocales"/>
    <inherits name="com.google.gwt.i18n.I18N"/>
    <extend-property name="locale" values="de,zh,es_ES,fr_FR,pt_BR,ru_RU"/>

    <inherits name="com.google.gwt.user.Debug"/>
    <set-property name="gwt.enableDebugId" value="false"/>
</module>